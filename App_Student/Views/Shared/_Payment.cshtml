@model App_Student.Models.Tbl_Transaction
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Reset_password</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">

</head>
<body id="paymentf">
    <div class="container-fluid">
        <div class="row" style="padding:50px;">
            <div class="col-md-6 offset-3">
                <div class="card">
                    <div class="card-header">
                        <h3 class="text-center text-primary">payment</h3>
                    </div>
                    <div class="card-body">
                        <div method="post" id="transaction_form">

                            <div  id="divnet">
                                <div class="form-group">
                                    <label asp-for="NETB_Bank_Account_No"></label>
                                    <input type="text" asp-for="NETB_Bank_Account_No" class="form-control" id="netaccount" />
                                    <span asp-validation-for="NETB_Bank_Account_No" id="netb" style="color:red"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="NETB_IFC_Cod"></label>
                                    <input type="text" asp-for="NETB_IFC_Cod" class="form-control" id="netifc" />
                                    <span asp-validation-for="NETB_IFC_Cod" id="idc" style="color:red"></span>
                                </div>
                            </div>

                            @* upi*@
                            <div class="form-group" id="divupi">
                                <label asp-for="Upi_Id"></label>
                                <input type="text" asp-for="Upi_Id" class="form-control" id="upi" />
                                <span asp-validation-for="NETB_IFC_Cod" id="up" style="color:red"></span>

                            </div>

                            @*cheqe*@
                            <div id="divcheque">
                                <div class="form-group" >
                                    <label asp-for="Checqe_No"></label>
                                    <input type="text" asp-for="Checqe_No" class="form-control" id="checkno" />
                                    <span asp-validation-for="NETB_IFC_Cod" id="cn" style="color:red"></span>

                                </div>

                                <div class="form-group">
                                    <label asp-for="Cheqe_Account_No"></label>
                                    <input type="text" asp-for="Cheqe_Account_No" class="form-control" id="chekaccount" />
                                    <span asp-validation-for="NETB_IFC_Cod" id="ca" style="color:red"></span>

                                </div>
                            </div>
                            @* case*@
                            <div class="form-group" id="divcash">
                                <label asp-for="Total_Cash"></label>
                                <input type="text" asp-for="Total_Cash" class="form-control" id="tcash" />
                                <span asp-validation-for="NETB_IFC_Cod" id="tc" style="color:red"></span>

                            </div>


                            <div class="form-group m-2">
                                <button class="btn btn-primary" type="button" id="btnpayment"> Save </button>
                            </div>
                        </div>
                    </div>

                </div>
            </div>


        </div>
    </div>
</body>
</html>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>
