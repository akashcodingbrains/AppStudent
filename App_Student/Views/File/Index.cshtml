@model IEnumerable<App_Student.Models.other.Tbl_file>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">

</head>
<body>
    <div class="container-fluid">
        <div class="row" style="padding:50px;">
            <div class="col-sm-4 offset-4">
                <input type="submit" value="submit" class="btn btn-primary float-end m-2" id="btn_add_data" />
                <div class="card">
                    <div class="card-header">
                        <h3 class="text-center text-primary">Add File</h3>
                    </div>
                    <div class="card-body">


                        <form id="form_data">
                            <table class="table table-striped">
                                <thead>
                                    <tr>

                                        <th>Id</th>
                                        <th>Name</th>
                                        <th>File_name</th>
                                      
                                        <th>Action</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @{

                                        if (Model != null)
                                        {
                                            foreach (var item in Model)
                                            {
                                            <tr>
                                            <td>@item.Id</td>
                                            <td>@item.Name</td>
                                            <td>@item.File_Name</td>
                                            <td>
                                            
                                            </td>
                                            </tr>    
                                            
                                            }

                                        }

                                       }
                                </tbody>

                            </table>
                            

                        </form>



                    </div>
                   
                </div>
            </div>


        </div>
    </div>
</body>
</html>

